<Window x:Class="DatEx.Skelya.GUI.Dialogs.ViewSnapshotDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatEx.Skelya.GUI.Dialogs"
        mc:Ignorable="d"
        Title="ViewSnapshotDialog" SizeToContent="Height" Width="1500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Image Grid.Column="0" Grid.Row="0" x:Name="Part_SnapshotImg" Stretch="Uniform"/>
    </Grid>
</Window>
